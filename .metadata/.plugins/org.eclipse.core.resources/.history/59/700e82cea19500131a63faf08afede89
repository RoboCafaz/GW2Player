package com.robocafaz.gwplayer.main.executor;

import java.awt.event.KeyEvent;
import com.robocafaz.gwplayer.main.keystroke.KeyPresser;
import com.robocafaz.gwplayer.main.song.note.Action;

public static class KeyboardExecutor extends AbstractExecutor {
  @Override
  public void execute(Action action) {
    switch (action) {
    case N1:
      KeyPresser.keyPress(KeyEvent.VK_1);
      break;
    case N2:
      KeyPresser.keyPress(KeyEvent.VK_2);
      break;
    case N3:
      KeyPresser.keyPress(KeyEvent.VK_3);
      break;
    case N4:
      KeyPresser.keyPress(KeyEvent.VK_4);
      break;
    case N5:
      KeyPresser.keyPress(KeyEvent.VK_5);
      break;
    case N6:
      KeyPresser.keyPress(KeyEvent.VK_6);
      break;
    case N7:
      KeyPresser.keyPress(KeyEvent.VK_7);
      break;
    case N8:
      KeyPresser.keyPress(KeyEvent.VK_8);
      break;
    case OCTAVE_DOWN:
      KeyPresser.keyPress(KeyEvent.VK_9);
      break;
    case OCTAVE_UP:
      KeyPresser.keyPress(KeyEvent.VK_0);
      break;
    default:
      break;
    }
  }
}
