package com.robocafaz.gwplayer.main.executor;

import java.awt.event.KeyEvent;
import com.robocafaz.gwplayer.main.keystroke.KeyPresser;
import com.robocafaz.gwplayer.main.song.note.Action;

public class KeyboardExecutor extends AbstractExecutor {
  @Override
  public void execute(Action action) {
    switch (action) {
    case N1:
      KeyPresser.keyPress(KeyEvent.VK_1);
      break;
    case N2:
      KeyPresser.keyPress(KeyEvent.VK_1);
      break;
    case N3:
      KeyPresser.keyPress(KeyEvent.VK_1);
      break;
    case N4:
      KeyPresser.keyPress(KeyEvent.VK_1);
      break;
    case N5:
      KeyPresser.keyPress(KeyEvent.VK_1);
      break;
    case N6:
      KeyPresser.keyPress(KeyEvent.VK_1);
      break;
    case N7:
      KeyPresser.keyPress(KeyEvent.VK_1);
      break;
    case N8:
      KeyPresser.keyPress(KeyEvent.VK_1);
      break;
    case OCTAVE_DOWN:
      KeyPresser.keyPress(KeyEvent.VK_1);
      break;
    case OCTAVE_UP:
      KeyPresser.keyPress(KeyEvent.VK_1);
      break;
    default:
      break;

    }
  }
}
